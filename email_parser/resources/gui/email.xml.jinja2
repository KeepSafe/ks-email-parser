<?xml version="1.0" encoding="UTF-8"?>
<resources xmlns:tools="http://schemas.android.com/tools" template="{{ template }}" style="{{ style }}">
{% for value in values %}
    {% if value[0] == 'subject' %}
    <string name="subject">{{ value[1]|e }}</string>
    {% elif value[0] in attrs %}
    <string name="{{ value[0]| replace('\r\n', '\n')}}" isText="false"><![CDATA[[[{{ value[1]| replace('\r\n', '\n') }}]]]]></string>
    {% else %}
    <string name="{{ value[0]| replace('\r\n', '\n') }}"><![CDATA[{{ value[1]| replace('\r\n', '\n') }}]]></string>
    {% endif %}
{% endfor %}
</resources>
